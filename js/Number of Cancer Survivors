<head>
<script type="text/javascript">
window.onload = function () {
	var chart = new CanvasJS.Chart("chartContainer",
	{
		title:{
			text: "Number of Cancer Survivors",
		},
		exportEnabled: true,
		axisY: {
			includeZero:false,
			title: "Number of Cancer Survivors",
			interval: 50,
		}, 
		axisX: {
			interval:10,
			title: "Age",
		},
		data: [
		{
			type: "rangeBar",
			showInLegend: true,
          //indexLabel: "{y[#index]}",
			legendText: "Department wise Min and Max Salary",
			dataPoints: [   // Y: [Low, High]
			
				
               {x: 30, y:[1,424], label: "65 Plus Years"},
			   {x: 50, y:[1, 190], label: "45-64 Years"},
               {x: 40, y:[1, 26], label: "Under 45 Years"},
              {x: 60,y:[1, 640], label: "All Ages" }
			]
		}
		]
	});
	chart.render();
}
</script>
<script type="text/javascript" src="/assets/script/canvasjs.min.js"></script>
</head>
<body>
<div id="chartContainer" style="height: 300px; width: 100%;"></div>
</body>


